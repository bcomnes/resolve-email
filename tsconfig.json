{
  "extends": "@voxpelli/tsconfig/node20.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "erasableSyntaxOnly": true,
    "allowImportingTsExtensions": true,
    "rewriteRelativeImportExtensions": true,
    "verbatimModuleSyntax": true,
    "module": "nodenext"
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules",
    "disposable-email-domains",
    "coverage"
  ]
}
